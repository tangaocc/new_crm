package cn.tencent.crm.domain;

public class Dept {
	//toString setter/getter 构造函数
		private Long id;
		private String name;
		public Long getId() {
			return id;
		}
		public void setId(Long id) {
			this.id = id;
		}
		public String getName() {
			return name;
		}
		public void setName(String name) {
			this.name = name;
		}
		
		public Dept() {
			super();
			// TODO Auto-generated constructor stub
		}
		public Dept(Long id, String name) {
			super();
			this.id = id;
			this.name = name;
		}
		@Override
		public String toString() {
			return "Department [id=" + id + ", name=" + name + "]";
		}
}
